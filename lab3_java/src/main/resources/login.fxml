<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="150.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.nsu.minesweeper.controller.LoginController">
   <children>
      <Button fx:id="exitButton" layoutX="165.0" layoutY="104.0" mnemonicParsing="false" prefHeight="30.0" prefWidth="91.0" text="EXIT">
         <font>
            <Font name="Consolas" size="16.0" />
         </font>
      </Button>
      <Button fx:id="enterButton" layoutX="43.0" layoutY="104.0" mnemonicParsing="false" prefHeight="28.0" prefWidth="91.0" text="ENTER">
         <font>
            <Font name="Consolas" size="16.0" />
         </font>
      </Button>
      <TextField fx:id="textArea" layoutX="43.0" layoutY="63.0" prefHeight="30.0" prefWidth="214.0" promptText="Your nickname" />
      <Label layoutX="107.0" layoutY="21.0" text="LOGIN">
         <font>
            <Font name="Consolas" size="31.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
